sample = ttw_jetClean_test
N = 32

+IsExpressJob = True
universe = vanilla
executable = run_2lss.sh
arguments = $(sample) $(Process)
output = condor_logs/$(sample).stdout.$(Process)
error = condor_logs/$(sample).stderr.$(Process)
should_transfer_files = yes
when_to_transfer_output = on_exit
log = condor_logs/logfile.log
on_exit_remove = (ExitBySignal == False) && (ExitCode == 0)
requirements = (machine != "skynet007.crc.nd.edu")
#requirements = (machine != "skynet007.crc.nd.edu" && machine != "d12chas563.crc.nd.edu" && machine != "d12chas564.crc.nd.edu" && machine != "d12chas579.crc.nd.edu" && machine != "d12chas584.crc.nd.edu" )
requestMemory=130
queue $(N)




